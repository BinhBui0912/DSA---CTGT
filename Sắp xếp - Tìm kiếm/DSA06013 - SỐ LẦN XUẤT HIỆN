#include <bits/stdc++.h>

using namespace std;

using ll = long long;

int main(){
	int t; cin >> t;
	while(t--){
		map<int, int> mp;
		int n, x;
		cin >> n >> x;
		int a[n];
		for(int i = 0; i < n; i++){
			cin >> a[i];
			mp[a[i]]++;
		}
		if(!mp[x]) cout << "-1" << endl;
		else cout << mp[x] << endl;
	}
}
