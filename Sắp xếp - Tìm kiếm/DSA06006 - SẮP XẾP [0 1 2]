#include <bits/stdc++.h>

using namespace std;

using ll = long long;

int main(){
	int t; cin >> t;
	while(t--){
		int n; cin >> n;
		int cnt[3] = {0};
		for(int i = 0; i < n; i++){
			int x; cin >> x;
			cnt[x]++;
		}
		for(int i = 0; i < 3; i++){
			for(int j = 0; j < cnt[i]; j++){
				cout << i << " ";
			}
		}
		cout << endl;
	}
}
