#include<bits/stdc++.h>

using namespace std;

using ll = long long;

int main(){
	int t; cin >> t;
	while(t--){
		int n; cin >> n;
		string s;
		cin >> s;
		if(next_permutation(s.begin(), s.end())){
			cout << n << " ";
			cout << s;
		} 
		else cout << n << " " << "BIGGEST";
		cout << endl;
	}
} 
